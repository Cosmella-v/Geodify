{
	"geode": "4.1.0",
	"gd": {
		"android": "2.2074",
		"win": "2.2074",
        "mac": "2.2074"
	},
	"version": "v2.0.0",
	"id": "omgrod.geodify",
	"name": "Geodify",
	"developers": ["OmgRod", "Viper"],
	"description": "Converts most menus' backgrounds to the Geode one (SwelvyBG).",
	"early-load": true,
	"resources": {
        "spritesheets": {
            "previewSheet": [
                "res/previews/*.png"
            ]
        },
        "files": [
            "res/layers.json"
        ],
        "sprites": [
            "res/*.png"
        ]
	},
	"settings": {
        "color-title": {
            "type": "title",
            "name": "Color Offset Settings"
        },
        "enable-color": {
            "name": "Enable Color Offset",
            "description": "Enables color offset for the SwelvyBG.",
            "type": "bool",
            "default": false
        },
        "color-0": {
            "name": "Color Offset - 1st",
            "description": "The color offset of the background.",
            "type": "color",
            "default": "#F4D48E",
            "enable-if": "enable-color"
        },
        "color-1": {
            "name": "Color Offset - 2nd",
            "description": "The color offset of the background.",
            "type": "color",
            "default": "#F5AE7D",
            "enable-if": "enable-color"
        },
        "color-2": {
            "name": "Color Offset - 3rd",
            "description": "The color offset of the background.",
            "type": "color",
            "default": "#EC897C",
            "enable-if": "enable-color"
        },
        "color-3": {
            "name": "Color Offset - 4th",
            "description": "The color offset of the background.",
            "type": "color",
            "default": "#D56985",
            "enable-if": "enable-color"
        },
        "color-4": {
            "name": "Color Offset - 5th",
            "description": "The color offset of the background.",
            "type": "color",
            "default": "#AD5492",
            "enable-if": "enable-color"
        },
        "color-5": {
            "name": "Color Offset - 6th",
            "description": "The color offset of the background.",
            "type": "color",
            "default": "#714A9A",
            "enable-if": "enable-color"
        },
        "gd/title": {
            "type": "title",
            "name": "Geometry Dash"
        },
        "gd/MenuLayer": {
            "name": "Show in main menu",
            "description": "MenuLayer",
            "type": "bool",
            "default": true
        },
        "gd/PlayLayer": {
            "name": "Show in gameplay",
            "description": "PlayLayer",
            "type": "bool",
            "default": false
        },
        "gd/LoadingLayer": {
            "name": "Show in loading screen",
            "description": "LoadingLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelSelectLayer": {
            "name": "Show in level select menu",
            "description": "LevelSelectLayer",
            "type": "bool",
            "default": true
        },
        "gd/CreatorLayer": {
            "name": "Show in creator menu",
            "description": "CreatorLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelBrowserLayer": {
            "name": "Show in level lists",
            "description": "LevelBrowserLayer",
            "type": "bool",
            "default": true
        },
        "gd/EditLevelLayer": {
            "name": "Show in edit level menu",
            "description": "EditLevelLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelInfoLayer": {
            "name": "Show in play level menu",
            "description": "LevelInfoLayer",
            "type": "bool",
            "default": true
        },
        "gd/GJGarageLayer": {
            "name": "Show in icon select menu",
            "description": "GJGarageLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelSearchLayer": {
            "name": "Show in search filters menu",
            "description": "LevelSearchLayer",
            "type": "bool",
            "default": true
        },
        "gd/LeaderboardsLayer": {
            "name": "Show in leaderboards menu",
            "description": "LeaderboardsLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelListLayer": {
            "name": "Show in level list menu",
            "description": "LevelListLayer",
            "type": "bool",
            "default": true
        },
        "gd/GauntletSelectLayer": {
            "name": "Show in gauntlet select menu",
            "description": "GauntletSelectLayer",
            "type": "bool",
            "default": true
        },
        "gd/GauntletLayer": {
            "name": "Show in gauntlet map menu",
            "description": "GauntletLayer",
            "type": "bool",
            "default": true
        },
        "gd/SecretRewardsLayer": {
            "name": "Show in secret chests menu",
            "description": "SecretRewardsLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelAreaInnerLayer": {
            "name": "Show in tower levels menu",
            "description": "LevelAreaInnerLayer",
            "type": "bool",
            "default": true
        },
        "gd/LevelAreaLayer": {
            "name": "Show in tower menu",
            "description": "LevelAreaLayer",
            "type": "bool",
            "default": true
        },
        "gd/SecretLayer": {
            "name": "Show in Vault",
            "description": "SecretLayer",
            "type": "bool",
            "default": true
        },
        "gd/SecretLayer2": {
            "name": "Show in Vault of Secrets",
            "description": "SecretLayer2",
            "type": "bool",
            "default": true
        },
        "gd/SecretLayer3": {
            "name": "Show in Basement",
            "description": "SecretLayer3",
            "type": "bool",
            "default": true
        },
        "gd/SecretLayer4": {
            "name": "Show in Chamber of Time",
            "description": "SecretLayer4",
            "type": "bool",
            "default": true
        },
        "mods-title": {
            "type": "title",
            "name": "Mods Settings"
        },
        "external-mods": {
            "name": "external mods",
            "description": "For external mod layers",
            "type": "bool",
            "default": true
        },
        "alphalaneous.level_info_backgrounds": {
            "name": "Level Info Backgrounds",
            "description": "Replace alphalaneous.level_info_backgrounds background.",
            "type": "bool",
            "default": false
        }
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"importance": "required",
			"version": ">=v1.12.0"
		},
		{
			"id": "alphalaneous.happy_textures",
			"importance": "recommended",
			"version": ">=1.3.18"
		}
	],
    "incompatibilities": [
        {
            "id": "undefined0.icon_ninja",
            "importance": "conflicting",
            "version": ">=v1.0.0"
        }
    ],
    "tags": ["content", "customization", "enhancement", "interface", "offline", "universal"],
    "links": {
        "community": "https://discord.gg/vK3DuqJwyW",
        "homepage": "https://omgrod.me",
        "source": "https://github.com/OmgRod/Geodify"
    },
    "issues": {
        "info": "Create an issue or create feature requests on the GitHub repository.",
        "url": "https://github.com/OmgRod/Geodify/issues"
    }
}
